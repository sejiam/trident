      <button class="btn-1 back-white2">Hot</button>
      <button class="btn-1 back-white2">New</button>
      <button class="btn-1 back-white2">Top</button>
    </nav>
    <section id="feed">
      <article class="border-3 shadow back-white2">
        <img src="" alt="" id="tumbnail" class="border-2" />
