    <div style="width: 30vw">
      <nav id="navigation">
        <button class="btn-1 back-white2">A</button>
        <button class="btn-1 back-white2">B</button>
        <button class="btn-1 back-white2">C</button>
        <button class="btn-1 back-orange0">+</button>
